<h1>登录</h1>

<div class="row">
	<div class="col-md-6">
		<%= form_for(:session,url: login_path) do |f| %>
			<%= f.label '用户名：'%>
			<%= f.text_field :name %><br/><br/>
			<%= f.label '密码：' %>
			<%= f.password_field :password %><br/><br/>
			<%= f.submit '登录' %>
		<% end %>
		
		<br/><%=link_to '注册新用户' ,new_user_path %>


	</div>
</div>

		